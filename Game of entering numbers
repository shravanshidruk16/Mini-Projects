import java.util.*;
public class Main{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        while(true){
            System.out.print("Enter a number: ");
            int num = sc.nextInt();
            if( (num%10) != 0){
                System.out.println("Entered number is: "+num);
                }
            else{
                System.out.println("Entered number is multiple of 10");
                break;
            }
        }
        
    }
}
